{
  "name": "frida-g",
  "version": "1.0.0",
  "description": "Frida scripts generate to one script.",
  "main": "./src/index.js",
  "bin": {
    "frida-g": "./src/index.js"
  },
  "scripts": {
    "test": "node ./src/index ./test/test.js",
    "test:dev": "node ./src/index ./test/test.js -o wtest_g.js -w",
    "help": "node ./src/index -h",
    "prepare": "git submodule init && git submodule update",
    "postinstall": "git clone https://github.com/pgw00k/FridaScript.git --depth 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pgw00k/frida-g.git"
  },
  "author": "Wings",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pgw00k/frida-g/issues"
  },
  "homepage": "https://github.com/pgw00k/frida-g#readme",
  "devDependencies": {
    "yargs": "^17.7.2"
  }
}